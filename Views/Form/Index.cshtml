@model List<EmailForm> 
@*<partial name="_Notifications" />*@
<h1>List of Registered Users</h1>
<table class="table table-bordered table-striped">
    <thead class="bg-primary">
    <tr>
        <th>ID</th>
        <th>Full Name</th>
       @* <th>Last Name</th>*@
        <th>Gender</th>
        <th>Email</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model.OrderBy(e=> e.ID)) {   
        <tr>
            <td>@obj.ID</td>
            <td>@obj.FullName</td>
           @* <td>@obj.LastName</td>*@
           <td>@obj.Gender</td>
            <td>@obj.Email</td>
            <td>
        <div class="w-50 btn-group" role="group">
                    <a asp-controller="Form" asp-action="Edit" asp-route-id="@obj.ID" class="btn btn-primary mx-2">
                        <i class="bi bi-pencil-square"></i>Edit
                    </a>
                    <a asp-controller="Form" asp-action="Delete" asp-route-id="@obj.ID" class="btn btn-danger mx-2">
                        <i class="bi bi-trash-fill"></i>Delete
                    </a>
                </div>   
                </td>
            </tr>
              }
    </tbody>

</table>