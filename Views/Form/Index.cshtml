@model List<EmailForm> 
<h1>List of Registered Users</h1>
<table class="table table-bordered table-striped">
    <thead class="bg-primary">
    <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Gender</th>
        <th>Email</th>
    </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model.OrderBy(e=> e.ID)){
        <tr>
            <td>@obj.ID</td>
            <td>@obj.FirstName</td>
            <td>@obj.LastName</td>
           <td>@obj.Gender</td>
            <td>@obj.Email</td>
        </tr>
        }
    </tbody>

</table>